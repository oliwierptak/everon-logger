[
    {
        "name": "StreamLoggerPluginConfigurator",
        "extends": "\\Everon\\Logger\\Configurator\\AbstractPluginConfigurator",
        "schema": [
            {
                "name": "pluginClass",
                "type": "string",
                "default": "\\Everon\\Logger\\Plugin\\Stream\\StreamLoggerPlugin::class"
            },
            {
                "name": "pluginFactoryClass",
                "type": "string"
            },
            {
                "name": "logLevel",
                "type": "string",
                "default": "debug"
            },
            {
                "name": "streamLocation",
                "type": "string"
            },
            {
                "name": "shouldBubble",
                "type": "bool",
                "default": true
            },
            {
                "name": "filePermission",
                "type": "int"
            },
            {
                "name": "useLocking",
                "type": "bool",
                "default": false
            }
        ]
    }
]
