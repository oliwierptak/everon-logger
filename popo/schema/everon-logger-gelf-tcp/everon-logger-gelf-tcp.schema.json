[
    {
        "name": "GelfTcpLoggerPluginConfigurator",
        "schema": [
            {
                "name": "logLevel",
                "type": "string",
                "default": "debug"
            },
            {
                "name": "shouldBubble",
                "type": "bool",
                "default": true
            },
            {
                "name": "host",
                "type": "string",
                "default": "\\Gelf\\Transport\\TcpTransport::DEFAULT_HOST"
            },
            {
                "name": "port",
                "type": "int",
                "default": "\\Gelf\\Transport\\TcpTransport::DEFAULT_PORT"
            },
            {
                "name": "sslOptions",
                "type": "GelfLoggerPluginSslOptions",
                "singular": "sslOption"
            },
            {
                "name": "useSsl",
                "type": "bool",
                "default": false
            }
        ]
    }
]
